{
  "name": "csvjs",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "main": "dist/csvjs.umd.js",
  "module": "dist/csvjs.esm.js",
  "typings": "dist/types/csvjs.d.ts",
  "files": [
    "dist"
  ],
  "author": "Khoi Pham <pckhoi@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint",
    "prebuild": "rimraf dist",
    "build": "tsc && rollup -c rollup.config.ts",
    "gendoc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage --useStderr",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "benchmark": "rollup -c benchmark.rollup.config.ts && npx eprint dist/benchmark.js",
    "benchmark-debug": "rollup -c benchmark.rollup.config.ts && npx eprint --debug dist/benchmark.js",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "verbose": false,
    "runner": "jest-electron/runner",
    "testEnvironment": "jest-electron/environment",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 85,
        "statements": 85
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^10.11.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-eslint": "^10.1.0",
    "benchmark": "^2.1.4",
    "colors": "^1.3.2",
    "commitizen": "^4.2.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eprint": "^0.1.2",
    "eslint": "^7.16.0",
    "husky": "^1.0.1",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "jest-electron": "^0.1.11",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "papaparse": "^5.3.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-base64": "^1.0.1",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "semantic-release": "^17.3.0",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^26.4.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.19.2",
    "typescript": "^3.0.3"
  },
  "dependencies": {}
}
